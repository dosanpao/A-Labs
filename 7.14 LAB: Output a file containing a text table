#include <iostream>
#include <fstream>
using namespace std;

int main() {

   /* Type your code here. */
   ofstream outFS;
   string fileName;
   cin >> fileName;
   int pairs;
   cin >> pairs;
   outFS.open(fileName);
   
   if (!outFS.is_open()) {
      cout << "error" << endl;
   }
   else {
      string name, age;
      outFS << "Name Age" << endl;
      for (int i = 0; i < pairs; i++) {
         cin >> name >> age;
         outFS << name << ' ' << age << endl;
      }
   }
   outFS.close();
   
   return 0;
}
