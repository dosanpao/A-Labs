#include <iostream>
using namespace std;

/* Define your function here */
int MaxMagnitude(int userVal1, int userVal2) {
   if (abs(userVal1) > abs(userVal2)) {
      return userVal1;
   }
   return userVal2;
}

int main() {
   /* Type your code here */
   int num1, num2;
   cin >> num1 >> num2;
   cout << MaxMagnitude(num1,num2) << endl;

   return 0;
}
