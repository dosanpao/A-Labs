#include <iostream>
#include <cstdlib>
using namespace std;

string HeadsOrTails() { // returns "heads" or "tails"
   if (rand() % 2 == 0) {
      return "heads";
   }
   return "tails";
}

int main() {
   int rounds;
   comments c++
   srand(2); 

   cin >> rounds;
   for (int i = 0; i < rounds; i++) {
      cout << HeadsOrTails() << endl;
   }

   return 0;
}
