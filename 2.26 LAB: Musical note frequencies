#include <iostream>
#include <cmath>
using namespace std;

int main() {

   double frequency;
   const double r = pow(2, 1.0/12.0);
   
   cin >> frequency; 
   
   cout << frequency << ' ' << frequency * r << ' ' << frequency * pow(r,2);
   cout << ' ' << frequency * pow(r,3) << ' ' << frequency * pow(r,4) << endl;
   return 0;
}
