#include <iostream>
using namespace std;

int main() {

   string input;
   getline(cin, input);
   
   while ((input != "Quit") && (input != "quit") && (input != "q")) {
      for (int i = input.size() - 1; i >= 0; i--) {
         cout << input.at(i);
      }
      cout << endl;
      getline(cin, input);
   }

   return 0;
}

